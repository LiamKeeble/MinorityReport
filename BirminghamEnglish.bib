@article{ ISI:000420174700005,
Author = {Dodd, William},
Title = {{UNDER PRESSURE? The Anglicisms Debate in Contemporary Germany as a
   Barometer of German National Identity Today}},
Journal = {{GERMAN POLITICS AND SOCIETY}},
Year = {{2015}},
Volume = {{33}},
Number = {{1-2, SI}},
Pages = {{58-68}},
Month = {{SPR-SUM}},
Abstract = {{This article reports on contemporary debates in Germany on the extensive
   use of English in Germans' use of German. In particular, it focuses on
   the debate held at the University of Birmingham between Professor Jurgen
   Schiewe and Thomas Paulwitz on the question: ``The influence of English
   on German today: Grounds for concern?{''} The rise of a nationalist
   discourse on language since the mid-1990s is traced with particular
   reference to the Verein Deutsche Sprache and the quarterly publication
   Deutsche Sprachwelt. The purist position represented by Paulwitz, editor
   of Deutsche Sprachwelt, and opposed by Schiewe, Professor of German
   Philology at the University of Greifswald, is found to represent a
   discourse on national identity that fails to engage with modern
   linguistic science.}},
