@inproceedings{ ISI:000235909900019,
Author = {Schmied, J},
Editor = {{Renouf, A and Kehoe, A}},
Title = {{New ways of analysing ESL on the WWW with WebCorp and WebPhraseCount}},
Booktitle = {{Changing Face of Corpus Linguistics}},
Series = {{LANGUAGE AND COMPUTERS : STUDIES IN PRACTICAL LINGUISTICS}},
Year = {{2006}},
Number = {{55}},
Pages = {{309-324}},
Note = {{24th International Computer Archive of Modern and Medieval English
   (ICAME), Guernsey, ENGLAND, APR 23-27, 2003}},
Abstract = {{This paper shows how software tools call be developed to interface with
   search engines and help linguists to make use of the world-wide web in
   their work. Two complementary tools ore explained and tested here,
   WebCorp, developed by the Research and Development Unit for English
   Studies, now in Birmingham, and WebPhraseCount, developed at Chemnitz.
   These tools can be used by advanced language learners as well as
   linguists interested in variation in English world-wide. This paper
