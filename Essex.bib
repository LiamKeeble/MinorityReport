@inproceedings{ ISI:000473545600007,
Author = {Camilleri, Maris and Sadler, Louisa},
Editor = {{Butt, M and King, TH}},
Title = {{Schematising (Morpho)Syntactic Change in LFG: Insights from
   grammaticalisation in Arabic}},
Booktitle = {{PROCEEDINGS OF THE LFG'18 CONFERENCE}},
Series = {{Proceedings of the LFG Conference}},
Year = {{2018}},
Pages = {{129-149}},
Note = {{Conference on Lexical Functional Grammar (LFG), Univ Vienna, Vienna,
   AUSTRIA, 2018}},
Abstract = {{This paper explores the grammaticalisation of two particular
   constructions in the Arabic vernaculars, seeking to provide an account
   of the relevant grammaticalisations paths and the commonality between
   then, using LFG as the theoretical model of morphosyntactic change. The
   two constructions express the PROGRESSIVE and the Universal PERFECT
   respectively. While their synchronic syntax has been recently analysed,
   here we address the task of exploring how hypotheses of reconstructed
   developmental paths that have led to the formation/grammaticalisation of
   these constructions could be accounted for by using the machinery of
   LFG. In particular, we observe how change does not necessarily
   constitute, or equate to, changes at the c-structure level.
   Alternatively, changes in function need not be accompanied by, or
   correlated with changes in form. While appreciating that the synchronic
   syntaxes of the two constructions under consideration are distinct, we
   observe how they share part of the developmental path that has led to
   their respective formation, and that is the shift from adjunction to
   embedding; a shift also observed in syntactic developments in
   Indo-European.}},
