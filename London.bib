@article{ ISI:000609629100023,
Author = {Moll, Richard J.},
Title = {{From Piccolomini to Sandford: Contextualizing Irony in ``The Originall
   of Herawldes{''}}},
Journal = {{MODERN PHILOLOGY}},
Year = {{2021}},
Volume = {{118}},
Number = {{3}},
Pages = {{418-431}},
Month = {{FEB 1}},
Abstract = {{In 1451, Eneas Sylvius Piccolomini claimed that heralds were first
   created and their rights established by Dionysus during his sojourn in
   India. The story was widely copied, printed, translated, and adapted
   well into the sixteenth century. While its original intent is uncertain,
   one early reader, Heinrich Cornelius Agrippa, used the story to ridicule
   heraldic claims to ancient authority. This is also true of James
   Sandford's translation of Agrippa's text, but a manuscript excerpt of
   the translation, which survives in London, British Library, Harley MS
   2076, changes the tone of the story. The unique context of ``The
   Originall of Herawldes{''} not only presents Piccolomini's text as a
   simple statement of heraldic rights and privileges, but the Harley text
   demonstrates that Sandford's work circulated in manuscript, perhaps
   before it was available in print.}},
