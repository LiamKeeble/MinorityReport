@article{ ISI:000645016000001,
Author = {Bolanos-Garcia-Escribano, Alejandro and Diaz-Cintas, Jorge and Massidda,
   Serenella},
Title = {{Latest advancements in audiovisual translation education INTRODUCTION}},
Journal = {{INTERPRETER AND TRANSLATOR TRAINER}},
Year = {{2021}},
Volume = {{15}},
Number = {{1, SI}},
Pages = {{1-12}},
Month = {{JAN 2}},
Abstract = {{The consumption of audiovisual content, from the more traditional
   animations, documentaries, movies, and TV shows to the more recent
   online user-generated content found on social media platforms, including
   video games, has grown exponentially over the last few decades. The
   omnipresence of screens in society has led to transformations in
   audiences' watching habits, now impatient to enjoy their programmes as
   soon as possible and inclined to binge watch. Recent technological
   advances in the production of specialist audiovisual translation (AVT)
   software and web-based applications have paved the way for further
   changes and enhancements in the ways professionals localise audiovisual
   content and in the nature of the services provided. This special issue
   sheds light on the current teaching and learning practices,
   methodologies and issues encountered by translator trainers specialised
   in AVT, with particular emphasis on pedagogical innovation, media
   accessibility, and translation technology.}},
