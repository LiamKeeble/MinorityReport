@article{ ISI:000458032900007,
Author = {Sakamoto, Akiko},
Title = {{Disruption in Translator -Client Matching: Paid Crowdsourcing Platforms
   vs Human Project Managers}},
Journal = {{TRADUMATICA-TRADUCCIO I TECNOLOGIES DE LA INFORMACIO I LA COMUNICACIO}},
Year = {{2018}},
Number = {{16}},
Pages = {{85-94}},
Month = {{DEC}},
Abstract = {{The paid crowdsourcing business model has brought a disruptive change to
   the translator procurement environment, offering clients algorithm-based
   automated systems as an alternative to conventional human-mediated
   project management services. This article analyses the
   conceptual/epistemological differences between the two from the
   viewpoint of knowledge management and considers the implications for
