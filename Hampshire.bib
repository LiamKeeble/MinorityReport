@article{ ISI:000245856400007,
Author = {Matsuda, Paul Kei and Tardy, Christine M.},
Title = {{Voice in academic writing: The rhetorical construction of author
   identity in blind manuscript review}},
Journal = {{ENGLISH FOR SPECIFIC PURPOSES}},
Year = {{2007}},
Volume = {{26}},
Number = {{2}},
Pages = {{235-249}},
Abstract = {{Some researchers have argued that voice is irrelevant to academic
   writing and that the importance of voice has been overstated in the
   professional literature {[}Helms-Park, R., \& Stapleton, P. (2003);
   Stapleton, P. (2002). To investigate whether and how a socially oriented
   notion of voice-defined as ``the amalgamative effect of the use of
   discursive and non-discursive features that language users choose,
   deliberately or otherwise, from socially available yet ever-changing
   repertoires{''} {[}Matsuda, P.K. (2001)-plays a role in academic
   writing, this study examined the construction of an author's discursive
   identity by peer reviewers in a simulated blind manuscript review
   process for an academic journal in the field of rhetoric and
   composition. The analysis of the written reviews as well as interviews
   with the two reviewers and the manuscript author indicated that the
   reviewers' constructions of the author's voice are related to their
   stance toward the author. The findings suggest that voice does play a
   role in academic writing and that there is a need for further research
   into the issue of identity construction from the perspectives of both
   writers and readers. (C) 2006 The American University. Published by
   Elsevier Ltd. All rights reserved.}},
