@article{ ISI:000615945100002,
Author = {Sampson, Rodney},
Title = {{PASCO > pais; etc.: Details of a phonetic change in Old French}},
Journal = {{REVUE DE LINGUISTIQUE ROMANE}},
Year = {{2020}},
Volume = {{84}},
Number = {{335}},
Pages = {{351+}},
Month = {{JUL-DEC}},
Abstract = {{For all the successes there have been in accounting for sound changes
   from Latin to Old French, certain problems still remain. One such
   concerns the reflex of word -final -sc after final rounded vowel
   deletion. It emerges in Old French as -is, as in PASCO > pais. To
   explain this development, three types of hypothesis have been advanced.
   Close analysis reveals that none is entirely satisfactory, although one
   offers great promise. After identifying its weak points, resolutions to
   these are proposed, yielding a robust and viable hypothesis.}},
