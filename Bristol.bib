@article{ ISI:000393024400004,
Author = {Mooney, Damien},
Title = {{Transmission and diffusion: Linguistic change in the regional French of
   Bearn}},
Journal = {{JOURNAL OF FRENCH LANGUAGE STUDIES}},
Year = {{2016}},
Volume = {{26}},
Number = {{3}},
Pages = {{327-352}},
Month = {{NOV}},
Abstract = {{This article examines the seemingly dichotomous linguistic processes of
   transmission and diffusion (Labov, 2007) in the regional variety of
   French spoken in Bearn, southwestern France. Using a sociophonetic
   apparent time methodology, an analysis of nasal vowel quality provides
   evidence for the advancement of linguistic changes from below taking
   place between successive generations during the transmission process, as
   well as for change from above taking place in the variety as a result of
   exposure to diffusing non-local varieties of French. The results address
   Labov's (2007) assertion that it is rare to investigate incremental
   changes occurring from below in European dialectological studies and
   shed light on the transmission-diffusion interface by showing the
   adoption of an individual change from above to instigate a
   faithfully-transmitted counterclockwise chain shift in the regional
   French nasal vowel system.}},
