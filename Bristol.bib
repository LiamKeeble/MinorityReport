@article{ ISI:000174022100012,
Author = {Beumers, B},
Title = {{Spinning the text: The play with infinity in contemporary Russian
   theatre}},
Journal = {{MODERN LANGUAGE REVIEW}},
Year = {{2002}},
Volume = {{97}},
Number = {{1}},
Pages = {{135-148}},
Month = {{JAN}},
Abstract = {{This article explores the tendency in contemporary Russian theatre
   towards a playful exploration of the text rather than its enactment.
   Infinite variations of the text contravene the finite and linear
   narrative. This playfulness is connected to chaos theory, where
   variations of patterns with slight divergences produce perfectly
   harmonious images: in both cases images of infinity (variation) are
   created within a finite space (form and stage). The article discusses in
   detail three directors: Anatolii Vasil'ev (Moscow), the leading theorist
   of contemporary Russian theatre; his pupil Klim who creates a space in
   which the actors can physically move through the text and engage in
   infinite play; and Vladimir Petrov (Omsk), who explores forms of ritual.}},
