@article{ ISI:000618833200002,
Author = {Helou, Leah B. and Gartner-Schmidt, Jackie L. and Hapner, Edie R. and
   Schneider, Sarah L. and Van Stan, Jarrad H.},
Title = {{Mapping Meta-Therapy in Voice Interventions onto the Rehabilitation
   Treatment Specification System}},
Journal = {{SEMINARS IN SPEECH AND LANGUAGE}},
