@inproceedings{ ISI:000082270100010,
Author = {Cutting, J},
Editor = {{Hunston, S}},
Title = {{Opening lines of questions from the floor}},
Booktitle = {{LANGUAGE AT WORK}},
Series = {{BRITISH STUDIES IN APPLIED LINGUISTICS}},
Year = {{1998}},
Volume = {{13}},
Pages = {{123-136}},
Note = {{Annual Meeting of the British-Association-for-Applied-Linguistics, UNIV
   BIRMINGHAM, BIRMINGHAM, ENGLAND, SEP, 1997}},
Organization = {{British Assoc Appl Linguist}},
Abstract = {{Discussion with conference participants whose first language is not
   English has revealed that they are often reluctant to ask questions from
   the floor in the discussions following papers. One of the main reasons
   for this reluctance appears to be lack of familiarity with linguistic
   mechanisms for opening their contribution. This paper describes the
   results of a detailed grammatical and lexical analysis of the opening
   lines of questions from the floor collected in language conferences
   (BAAL, Sociolinguistics Symposium and the British Association of
   Lecturers in English for Academic Purposes) from 1993 to 1997. The
   findings show that native-speaker-of-English participants frequently
   indicate the specific topic, and hedge, adding doubt and using fillers.
   They quite often use cooperative metalanguage to categorise their
   contribution, minimise the imposition by promising to be brief,
   impersonalise by distancing themselves using a past tense and hedge by
   mitigating with adjuncts. Analysis takes into account gender and
   conference type.
   EFL course books treat the subject of conference language in a
   superficial way not based on corpus analysis. A suggestion is made as to
   how EAP and ESP students could be trained to see conventions of hedging
   and mitigation as rules of spoken discourse particular to this context,
   and to use them effectively.}},
