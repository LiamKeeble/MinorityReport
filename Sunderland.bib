@article{ ISI:A1991GZ66600001,
Author = {SUNDERLAND, J},
Title = {{THE DECLINE OF MAN}},
Journal = {{JOURNAL OF PRAGMATICS}},
Year = {{1991}},
Volume = {{16}},
Number = {{6}},
Pages = {{505-522}},
Month = {{DEC}},
Abstract = {{This article looks at diachronic change in the meaning, use and
   interpretation of man, men, a man and man compounds in the `generic
   masculine' sense. I suggest that during the last two decades, in the
   context of the on-going `sexist language' debate, deliberate
   intervention has influenced the meaning, use and interpretation of
   `man-words'. I am specifically suggesting that largely due to this
   intervention, these words, which once had a fully generic denotative
   meaning (though this may have anyway been becoming less and less
   generic) now in many contexts have relatively little generic potential,
   and that their `genericity' is on the whole in a state of decline. Man
   cannot, however, be said to have only sex-specific potential.}},
