@article{ ISI:000462549100004,
Author = {Hyland, Ken and Jiang, Feng (Kevin)},
Title = {{Points of Reference: Changing Patterns of Academic Citation}},
Journal = {{APPLIED LINGUISTICS}},
Year = {{2019}},
Volume = {{40}},
Number = {{1}},
Pages = {{64-85}},
Month = {{FEB}},
Abstract = {{In this article we explore the ways in which academic citation practices
   have changed over the past 50 years. Based on the analysis of a corpus
   of 2.2 million words from the same leading journals in four disciplines
   in 1965, 1985, and 2015, we document a substantial rise in citations
   over the period, particularly in applied linguistics and sociology. This
   is partly because there is now so much more research to report and that
   recognizing previous work is much easier as a result of electronic
   access and hyperlinks to sources. But citation is also increasingly
   required as a means of appropriately embedding research more securely in
   disciplinary understandings. Our results also show a fall in the use of
   reporting structures, a growing preference for non-integral forms, for
   research verbs, for the present tense, and for non-evaluative structures
   when reporting others' research. While patterns differ by discipline,
   there is a general trend towards writers suppressing human agency in
   knowledge-making and emphasizing the reported studies rather than those
   who conducted them to show how earlier research supports their own.}},
