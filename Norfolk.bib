@inproceedings{ ISI:A1996BJ20B00308,
Author = {Wang, XH and Zahorian, SA and Auberg, S},
Editor = {{Bunnell, HT and Idsardi, W}},
Title = {{Analysis of speech segments using variable spectral/temporal resolution}},
Booktitle = {{ICSLP 96 - FOURTH INTERNATIONAL CONFERENCE ON SPOKEN LANGUAGE
   PROCESSING, PROCEEDINGS, VOLS 1-4}},
Year = {{1996}},
Pages = {{1221-1224}},
Note = {{4th International Congress on Spoken Language Processing, PHILADELPHIA,
   PA, OCT 03-06, 1996}},
Organization = {{Univ Delaware; Alfred I DuPont Inst; Acoust Soc Amer; Acoust Soc Japan;
   Amer Speech Language Hearing Assoc; Austr Speech Sci \& Technol Assoc;
   European Speech Commun Assoc; IEEE Signal Proc Soc; Inc Canadian Acoust
   Assoc; Int Phonet Assoc; Linguist Soc Amer}},
Abstract = {{In this paper we present an approach for efficiently computing a compact
   temporal/spectral feature set for representing a segment of speech, with
   effective resolution depending on both frequency and time position
   within the segment. The goal is to mimic the resolution properties of
   the human auditory system, but using a computationally efficient
   FFT-based front end rather than a more complex auditory model. In
   particular we apply both frequency and time `'warping'' to FFT spectra
   to obtain good frequency resolution at low frequencies and good time
   resolution at high frequencies. Time resolution is also varied so that
   the center of the segment is better represented than the endpoints. The
   resolution can be varied by the selection of `'warping'' functions
   controlled using a small number of parameters. The method was
   experimentally verified for the classification of six stops extracted
   from the TIMIT continuous speech data base. The best classification rate
   obtained was 81.2\% for test data using 50 features computed with the
   method presented.}},
