@article{ ISI:000387160000009,
Author = {Guillot, Marie-Noelle},
Title = {{Cross-cultural pragmatics and audiovisual translation}},
Journal = {{TARGET-INTERNATIONAL JOURNAL OF TRANSLATION STUDIES}},
Year = {{2016}},
Volume = {{28}},
Number = {{2}},
Pages = {{288-301}},
Abstract = {{In this article, audiovisual translation (AVT) is considered
   contrastively from a cross-cultural pragmatics perspective, in its uses
   of language across languages and cultures. This inevitably broaches
   questions of linguistic and cultural representation, critical in a world
   in which the global availability of cultural products is ever greater.
   They are a main focus in this paper, with related questions about the
   development of subtitling and dubbing language as idiosyncratic
   varieties and expressive media, and implications for representation and
   its impact on audiences. AVT research has had many challenges to
   confront in its early days and these are relatively uncharted
   territories. Yet current developments like fansubbing and other
   crowdsourcing activities are re-defining the name of the game and
   heralding significant changes, in AVT practices and in the ways they and
   the products and responses they generate are accounted for in research
   (as evidenced in emerging re-evaluations of quality and subjectivity,
   e.g.; see Pwrez-Gonzalez 2012, 2014). These are central concerns in
   mapping the way forward.}},
