@article{ ISI:000468172800003,
Author = {Whiteside, Karin and Wharton, Sue},
Title = {{Semantic patterning of grammatical keywords in undergraduate academic
   writing from two close disciplines}},
Journal = {{JOURNAL OF ENGLISH FOR ACADEMIC PURPOSES}},
Year = {{2019}},
Volume = {{39}},
Pages = {{1-20}},
Month = {{MAY}},
Abstract = {{Contributing to the growing body of research on student texts, and the
   construct of disciplinarity at undergraduate level, this study involves
   a lexico-grammatical analysis of successful third-year student writing
   in two UK HE institutions from the disciplines of History and of
   Politics and International Relations. We use an inductive corpus
   approach in which the phraseological patterning around five grammatical
   keywords (of, and, that, as and this) is categorised according to
   semantic purpose. More similarities than variations were found across
   the four disciplinary/institutional sub-corpora, and, to a large extent,
   these similarities can be explained in terms of the shared foci of
   student Humanities and Social Sciences writing. From a practitioner
   perspective, we argue that the approach uncovers meaning-focused
   lexico-grammatical features of very high value due to their extremely
   regular occurrence within student texts. With regard to the EGAP versus
   ESAP debate, we suggest that an argument can be made for the teaching of
   `meta-disciplinary' literacy skills. (C) 2019 Elsevier Ltd. All rights
   reserved.}},
