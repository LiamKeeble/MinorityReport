@article{ ISI:000255085800004,
Author = {Martin, Catherine},
Title = {{The gift of the poem: Mallarme and Robert Duncan's `Ground Work - Before
   the War'}},
Journal = {{MODERN LANGUAGE REVIEW}},
Year = {{2008}},
Volume = {{103}},
Number = {{2}},
Pages = {{364+}},
Month = {{APR}},
Abstract = {{Robert Duncan's poetry is conventionally read in the context of the
   Black Mountain school of poetry or the San Francisco Renaissance of the
   1950s. However, his work is also deeply involved with that of the French
   symbolist Stephane Mallarme. While Mallarme informs much of Duncan's
   writing, notably the metaphors of interruption and dictation central to
   his poetics, a cluster of Duncan's poems from the mid-70s gives us an
   important insight into the procedures of his final Ground Work volumes,
   illuminating his changing notion of the poem as `gift'.}},
