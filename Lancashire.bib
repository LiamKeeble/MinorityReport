@inproceedings{ ISI:A1996BJ20B00586,
Author = {Humphries, JJ and Woodland, PC and Pearce, D},
Editor = {{Bunnell, HT and Idsardi, W}},
Title = {{Using accent-specific pronunciation modelling for robust speech
   recognition}},
Booktitle = {{ICSLP 96 - FOURTH INTERNATIONAL CONFERENCE ON SPOKEN LANGUAGE
   PROCESSING, PROCEEDINGS, VOLS 1-4}},
Year = {{1996}},
Pages = {{2324-2327}},
Note = {{4th International Congress on Spoken Language Processing, PHILADELPHIA,
   PA, OCT 03-06, 1996}},
Organization = {{Univ Delaware; Alfred I DuPont Inst; Acoust Soc Amer; Acoust Soc Japan;
   Amer Speech Language Hearing Assoc; Austr Speech Sci \& Technol Assoc;
   European Speech Commun Assoc; IEEE Signal Proc Soc; Inc Canadian Acoust
   Assoc; Int Phonet Assoc; Linguist Soc Amer}},
Abstract = {{A method of modelling accent-specific pronunciation variations is
   presented. Speech from an unseen accent group is phonetically
   transcribed such that pronunciation variations may be derived. These
   context-dependent variations are clustered in a decision tree which is
   used as a model of the pronunciation variation associated with this new
   accent group. The tree is then used to build a new pronunciation
   dictionary for use during the recognition process. Experiments are
   presented for the recognition of Lancashire \& Yorkshire accented speech
   using a recognizer trained on London \& South East England speakers. The
   results show that the addition of accent-specific pronunciations can
   reduce the error rate by almost 20\% for cross accent recognition. It is
   also shown that worthwhile gains in performance can be obtained using
   only a small amount of accent-specific data.}},
