@article{ ISI:000240755300013,
Author = {Peppe, Sue and McCann, Joanne and Gibbon, Fiona and O'Hare, Anne and
   Rutherford, Marion},
Title = {{Assessing prosodic and pragmatic ability in children with
   high-functioning autism}},
Journal = {{JOURNAL OF PRAGMATICS}},
