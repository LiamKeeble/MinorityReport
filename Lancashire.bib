@article{ ISI:000240755300005,
Author = {van der Kooij, Els and Crasborn, Onno and Emmerik, Wim},
Title = {{Explaining prosodic body leans in sign language of the Netherlands:
   Pragmatics required}},
Journal = {{JOURNAL OF PRAGMATICS}},
Year = {{2006}},
Volume = {{38}},
