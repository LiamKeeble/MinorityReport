@inproceedings{ ISI:A1997BJ45F00008,
Author = {Milroy, L},
Editor = {{Chu, A and Guerra, AMP and Tetreault, C}},
Title = {{The prepausal constraint in Tyneside English: A discourse level
   mechanism of linguistic change}},
Booktitle = {{SALSA IV - PROCEEDINGS OF THE FOURTH ANNUAL SYMPOSIUM ABOUT LANGUAGE AND
   SOCIETY - AUSTIN}},
Series = {{TEXAS LINQUISTIC FORUM (TLF)}},
Year = {{1997}},
Number = {{37}},
Pages = {{88-99}},
Note = {{4th Annual Symposium About Language and Society - Austin (SALSA IV),
   AUSTIN, TX, APR 12-14, 1996}},
Organization = {{Capitol Beverage Co; El Galindo; El Lago; Rio Diablo; Timpone's}},
Abstract = {{This paper arises from a more extensive variationist analysis of
   glottalization phenomena in the English of Tyneside in the North East of
   England. It focuses on the exceptions to the Prepausal Constraint (i.e.
   glottalized realizations of /t/ in prepausal contexts) which appear
   chiefly in the speech of younger working-class women, and are associated
   chiefly with the utterance-final grammatical tags and that, and all
   that, innit ('isn't it'), and wannit ('wasn't it'). It is argued that an
   analysis of conversational structure and conversational management
   procedures can contribute to an illuminating account of the PPC and its
   exceptions. The findings reported here are particularly relevant to an
   understanding of the spread of a linguistic change through the system at
   the level of conversational interaction.}},
