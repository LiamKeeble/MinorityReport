Author = {Elliott, J. M. and Elliott, J. A. and Hilton, J.},
Title = {{Sources of variation in post-Chernobyl radiocaesium in brown trout,
   Salmo trutta L., and Arctic charr, Salvelinus alpinus (L.), from six
   Cumbrian lakes (northwest England)}},
Journal = {{ANNALES DE LIMNOLOGIE-INTERNATIONAL JOURNAL OF LIMNOLOGY}},
Year = {{1993}},
Volume = {{29}},
