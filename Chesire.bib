@article{ ISI:000438188000002,
Author = {Schleef, Erik and Turton, Danielle},
Title = {{Sociophonetic variation of like in British dialects: effects of
   function, context and predictability}},
Journal = {{ENGLISH LANGUAGE \& LINGUISTICS}},
Year = {{2018}},
Volume = {{22}},
