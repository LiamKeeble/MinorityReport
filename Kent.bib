@article{ ISI:000236055900005,
Author = {Yunusova, Y and Weismer, G and Kent, RD and Rusche, NM},
Title = {{Breath-group intelligibility in dysarthria: Characteristics and
   underlying correlates}},
Journal = {{JOURNAL OF SPEECH LANGUAGE AND HEARING RESEARCH}},
Year = {{2005}},
Volume = {{48}},
Number = {{6}},
Pages = {{1294-1310}},
