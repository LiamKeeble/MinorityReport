@article{ ISI:000253253400002,
Author = {Lin, Mei-Chen and Harwood, Jake and Hummert, Mary Lee},
Title = {{Young adults' intergenerational communication schemas in Taiwan and the
   USA}},
Journal = {{JOURNAL OF LANGUAGE AND SOCIAL PSYCHOLOGY}},
Year = {{2008}},
Volume = {{27}},
Number = {{1}},
Pages = {{28-50}},
Month = {{MAR}},
Abstract = {{The present research systematically examines cognitive representations
   of stereotypes of old age and young adults' and older adults'
   perceptions of their own communication accommodative behaviors,
   emotions, and communication satisfaction when a particular
   Intergenerational Communication Schema is activated. Taiwanese
   respondents show both congruent and incongruent patterns of responding:
   Some schemas yield consistently positive or negative responses across
   dependent variables, whereas others yield mixed and apparently
   inconsistent patterns. Respondents from the USA show that a helping
   orientation, when it is associated with different schemas (positive,
   very sympathetic, or negative) may result in different overall
   evaluations of the intergenerational conversation experience. Results
   are discussed in relation to the Communication Predicament of Aging
   Model and Chinese cultural values of intergenerational communication.
   Limitations are acknowledged and future studies are suggested.}},
