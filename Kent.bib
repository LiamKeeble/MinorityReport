@article{ ISI:000551487300009,
Author = {Binger, Cathy and Kent-Walsh, Jennifer and Harrington, Nancy and
   Hollerbach, Quinn C.},
Title = {{Tracking Early Sentence-Building Progress in Graphic Symbol
   Communication}},
