@article{ ISI:000454690800001,
Author = {Haustein, Katja},
Title = {{HOW TO BE ALONE WITH OTHERS: PLESSNER, ADORNO, AND BARTHES ON TACT}},
Journal = {{MODERN LANGUAGE REVIEW}},
Year = {{2019}},
Volume = {{114}},
Number = {{1}},
Pages = {{1+}},
Month = {{JAN}},
Abstract = {{This article looks at three rarely compared theories of tact. Although
   developed during different periods of radical change in the twentieth
   century, they are based on the same diagnosis: the key problem of modern
   subjectivity is not the increased distance between individuals, but, on
   the contrary, its disappearance. Tact, understood as an ongoing
   negotiation between the demands of convention and the `unruly claims of
   the individual' (Adorno), emerges as a figure of distance, and of
   digression, that not only serves to describe the relations between
   individuals, or the individual within the group, but also facilitates
   alternative forms of critical enquiry.}},
