@article{ ISI:A1992JR89700016,
Author = {BEDROSIAN, JL and HOAG, LA and CALCULATOR, SN and MOLINEUX, B},
Title = {{VARIABLES INFLUENCING PERCEPTIONS OF THE COMMUNICATIVE COMPETENCE OF AN
   ADULT AUGMENTATIVE AND ALTERNATIVE COMMUNICATION-SYSTEM USER}},
Journal = {{JOURNAL OF SPEECH AND HEARING RESEARCH}},
Year = {{1992}},
Volume = {{35}},
Number = {{5}},
Pages = {{1105-1113}},
Month = {{OCT}},
Abstract = {{The effects of aided message length, partner reauditorization, and
   observer background on perceptions of the communicative competence of an
   adult augmentative and alternative communication (AAC) system user were
   examined. Two groups of subjects participated: naive adults with minimal
   exposure to nonspeaking persons, and speech-language pathologists
   currently working with AAC users. Four scripted videotaped
   conversational conditions involving an AAC user and a normally speaking
   partner were employed to manipulate aided message length and partner
   reauditorization. A balanced incomplete block design was used. Following
   each viewing, subjects completed a questionnaire designed to assess the
   communicative competence of the AAC user. Results indicated a
   significant interaction effect involving subject group and aided message
   length. The speech-language pathologists were affected by aided message
